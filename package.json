{
  "name": "3h-validate",
  "version": "0.4.0",
  "description": "A runtime validator.",
  "module": "./dist/3h-validate.min.js",
  "main": "./dist/3h-validate.umd.min.js",
  "types": "./index.d.ts",
  "author": "hhh",
  "license": "MIT",
  "scripts": {
    "prebuild": "tsc",
    "build": "rollup -c",
    "postbuild": "terser dist/3h-validate.js --config-file terser.json --module -o dist/3h-validate.min.js && terser dist/3h-validate.umd.js --config-file terser.json -o dist/3h-validate.umd.min.js && node stat",
    "test": "node ./test/index.js",
    "docs": "dts2md \"**\" \"!index.d.ts\" -i ./types -o ./docs -l -I Home.md"
  },
  "repository": "huang2002/3h-validate",
  "keywords": [
    "runtime",
    "validate",
    "validator",
    "validation",
    "check",
    "object",
    "type"
  ],
  "files": [
    "dist",
    "types",
    "index.d.ts"
  ],
  "devDependencies": {
    "@babel/core": "^7.17.0",
    "@babel/preset-env": "^7.16.0",
    "@rollup/plugin-babel": "^5.3.0",
    "3h-test": "^0.8.0",
    "dts2md": "^0.4.0",
    "rollup": "^2.70.0",
    "terser": "^5.12.0",
    "typescript": "^4.6.0"
  },
  "dependencies": {
    "3h-utils": "^0.5.0"
  }
}
